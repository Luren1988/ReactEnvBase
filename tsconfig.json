      "strictNullChecks": true,
      "noUnusedLocals": true,
      "noImplicitThis": true,
      "alwaysStrict": true,
      "sourceMap": true,
      "noImplicitAny": true,
      "lib": ["dom", "es2017"],
      "module": "es2015",
      "target": "es5",
